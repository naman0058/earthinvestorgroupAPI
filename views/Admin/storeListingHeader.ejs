}
.card-bodys {
 
  padding: 40px 40px 60px;
  border-radius: 10px;
  background: white;
  box-shadow: -5px -5px 10px rgba(255, 255, 255, 0.05),
    5px 5px 15px rgba(0, 0, 0, 0.5);
}
.form h2 {
  color: #c7c7c7;
  font-weight: 500;
  text-transform: uppercase;
  letter-spacing: 4px;
}


.card  .inputBox label {
  color: #868686;
  margin-bottom: 5px;
  font-size: 18px;
}
.card .input .inputBox input {
    height: 50px;
 
  color: white;
  background: #131419;
  border: none;
  outline: none;
  border-radius: 20px;
  padding: 5px 15px;
  font-size: 18px;
  box-shadow: inset -2px -2px 6px rgba(255, 255, 255, 0.1),
    inset 2px 2px 6px rgba(0, 0, 0, 0.8);
}
.card .input .inputBox input[type="submit"] {
  margin-top: 20px;
  box-shadow: -2px -2px 6px rgba(255, 255, 255, 0.1),
    2px 2px 6px rgba(0, 0, 0, 0.8);
}
.card .input .inputBox input[type="submit"]:active {
  color: white;
  margin-top: 20px;
  box-shadow: inset -2px -2px 6px rgba(255, 255, 255, 0.1),
    inset 2px 2px 6px rgba(0, 0, 0, 0.8);
}
.card .input .inputBox input::placeholder {
  color: #555;
  font-size: 18px;
}
.forget {
  margin-top: 30px;
  color: #555;
}
.forget a {
  color: #ff0047;
}

.text{
  font-size: 25px;
  font-weight: 600;
  margin-bottom: 35px;
